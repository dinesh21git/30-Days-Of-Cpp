#include <iostream>
using namespace std;

int main(){
 int year;
 bool is_leap;
 
 cout << "Enter an year to check is leap or not: ";
 cin >> year;

    if (year % 400 == 0){
        is_leap = true;
    }else if (year % 4 == 0 && year % 100 != 0) {
        is_leap =true;
    }
    
    if (is_leap) {
        cout << "Leap Year";
    } else {
        cout << "Non Leap Year";
    }

    return 0;
}
